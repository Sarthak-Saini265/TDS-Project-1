# Environment Variables Template
# Copy this to .env and fill in your actual values

# Your secret from the Google Form submission
STUDENT_SECRET=your-secret-here

# GitHub Personal Access Token (needs repo and admin:public_key permissions)
GITHUB_TOKEN=ghp_xxxxxxxxxxxxx

# Your GitHub username
GITHUB_USERNAME=your-username

# Google Gemini API Key
GEMINI_API_KEY=xxxxxxxxxxxxx

# AIpipe API Token (alternative to Gemini)
AIPIPE_TOKEN=xxxxxxxxxxxxx

# Use AIpipe instead of Gemini (set to true to enable)
USE_AIPIPE=false

# Port for Flask server (optional, defaults to 5000)
PORT=5000
